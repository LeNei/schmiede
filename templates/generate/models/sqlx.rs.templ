use chrono::{offset::Utc, DateTime};
use serde::{Serialize, Deserialize};
use sqlx::FromRow;


#[derive(FromRow, Serialize, Clone, Debug)]
#[serde(rename_all = "camelCase")]
pub struct {{ struct_name }} {
{%- match id %}
    {%- when IDType::Uuid %}
    id: uuid::Uuid,
    {%- when IDType::Int %}
    id: i32,
    {%- when IDType::None %}
{%- endmatch %}
    {%- for row in rows %}
    {{ row }},
    {%- endfor %}
    created_at: DateTime<Utc>,
    updated_at: DateTime<Utc>,
}
